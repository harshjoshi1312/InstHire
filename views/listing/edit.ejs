      <input name="new[role]" value="<%= listing.role %>" type="text" />
      <br /><br />
      <input name="new[description]" value="<%= listing.description %>" type="text" />
      <br /><br />
      <input name="new[image]" value="<%= listing.image %>" type="text" />
      <br /><br />
      <input name="new[location]" value="<%= listing.location %>" type="text" />
      <br /><br />
      <input name="new[country]" value="<%= listing.country %>" type="text" />
      <br /><br />
      <button>Edit your profile</button>
  </body>
</html>
